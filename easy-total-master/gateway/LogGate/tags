DEPS	Makefile	/^DEPS = $(OBJECTS:%.o=.%.d)$/;"	m
OBJECTS	Makefile	/^OBJECTS = ListenThread.o LogGate.o NetProcessor.o TaskThread.o Socket.o ServerConfig.o $/;"	m
SRCS	Makefile	/^SRCS = $(OBJECTS:%.o=%.cpp)$/;"	m
ServerConfig	ServerConfig.cpp	/^ServerConfig()$/;"	f
ServerConfig	ServerConfig.h	/^class ServerConfig : public xSingleton<ServerConfig>$/;"	c
ServerInfo	ServerConfig.h	/^        ServerInfo() : addr(AF_INET)$/;"	f	struct:ServerInfo
ServerInfo	ServerConfig.h	/^struct ServerInfo$/;"	s
TARGET	Makefile	/^TARGET = LogGate$/;"	m
addr	ServerConfig.h	/^        DoubleNetAddr addr;$/;"	m	struct:ServerInfo
getAddrByTag	ServerConfig.h	/^        NetAddr* getAddrByTag(std::string tag)$/;"	f	class:ServerConfig
getLocalAddr	ServerConfig.h	/^        NetAddr* getLocalAddr() { return localAddr.getAddr(); }$/;"	f	class:ServerConfig
loadConfig	ServerConfig.cpp	/^bool ServerConfig::loadConfig()$/;"	f	class:ServerConfig
localAddr	ServerConfig.h	/^        ServerInfo localAddr;$/;"	m	class:ServerConfig
serverList	ServerConfig.h	/^        std::map<std::string, ServerInfo> serverList;$/;"	m	class:ServerConfig
tag	ServerConfig.h	/^        std::string tag;$/;"	m	struct:ServerInfo
~ServerConfig	ServerConfig.cpp	/^~ServerConfig()$/;"	f
